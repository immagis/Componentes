      </Unit1>
      <Unit2>
        <Filename Value="..\..\Units\PersonBO.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="PersonBO"/>
      </Unit2>
    </Units>
  </ProjectOptions>
  <CompilerOptions>
    <Version Value="8"/>
    <PathDelim Value="\"/>
    <Target>
      <Filename Value="SimpleOPF"/>
    </Target>
    <SearchPaths>
      <OtherUnitFiles Value="..\..\Units\"/>
      <UnitOutputDirectory Value="..\..\..\..\bin\Laz\Demos\SimpleOPF\$(TargetCPU)-$(TargetOS)"/>
    </SearchPaths>
    <Linking>
      <Options>
        <Win32>
          <GraphicApplication Value="True"/>
        </Win32>
      </Options>
    </Linking>
    <Other>
      <CompilerMessages>
        <UseMsgFile Value="True"/>
      </CompilerMessages>
      <CompilerPath Value="$(CompPath)"/>
    </Other>
  </CompilerOptions>
</CONFIG>
